    <div class="col-md-6">

        <div class="app-contact-way box">
            <md-icon class="app-contact-way-icon">email</md-icon>
            <h4>@SharedLocalizer["EmailAddress"]</h4>
            <p>
                info@strawberrysass.com<br/>
                support@strawberrysass.com
            </p>
