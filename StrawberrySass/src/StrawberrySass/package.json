{
  "name": "strawberrysass.org",
  "version": "1.0.0",
  "description": "Website for Strawberry",
  "keywords": [
    "asp.net core",
    "angular 2",
    "typescript",
    "sass",
    "material design"
  ],
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "license": "MIT",
  "contributors": [
    "Maxime Glinas <maxime.gelinas@idmobiles.com>"
  ],
  "main": "Program.cs",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "postinstall": "npm run typings",
    "build": "cpx \"./UI/**/*.{html,png,jpg}\" ./wwwroot && tsc -p ./tsconfig.json",
    "build:watch": "concurrently \"cpx \"./UI/**/*.{html,png,jpg}\" ./wwwroot -w\" \"tsc -p ./tsconfig.json -w\"",
    "clean": "rimraf ./wwwroot",
    "typings": "typings install",
    "vendor": "npm run vendor:angular && npm run vendor:angular2-material && npm run vendor:core-js && npm run vendor:reflect-metadata && npm run vendor:rxjs && npm run vendor:systemjs && npm run vendor:zone.js",
    "vendor:angular": "cpx \"./node_modules/@angular/*/bundles/*.min.js\" ./wwwroot/Vendor/@angular",
    "vendor:angular2-material": "cpx \"./node_modules/@angular2-material/*/*.umd.js\" ./wwwroot/Vendor/@angular2-material",
    "vendor:core-js": "cpx \"./node_modules/core-js/client/shim.min.js\" ./wwwroot/Vendor/core-js/client",
    "vendor:reflect-metadata": "cpx \"./node_modules/reflect-metadata/Reflect.js\" ./wwwroot/Vendor/reflect-metadata",
    "vendor:rxjs": "cpx \"./node_modules/rxjs/**/*.js\" ./wwwroot/Vendor/rxjs",
    "vendor:systemjs": "cpx \"./node_modules/systemjs/dist/system.src.js\" ./wwwroot/Vendor/systemjs/dist",
    "vendor:zone.js": "cpx \"./node_modules/zone.js/dist/zone.min.js\" ./wwwroot/Vendor/zone.js/dist"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular2-material/button": "^2.0.0-alpha.8-2",
    "@angular2-material/core": "^2.0.0-alpha.8-2",
    "@angular2-material/toolbar": "^2.0.0-alpha.8-2",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "concurrently": "^2.2.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  }
}
